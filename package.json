{
  "name": "gymy",
  "version": "1.8.0",
  "description": "A gym tracker app",
  "productName": "Gymy",
  "author": "Mathis Gasparotto <mathis.gasparotto@hotmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "npx quasar dev -m pwa",
    "dev:android": "npm run dev:capacitor:android:ide",
    "dev:capacitor:android": "npx quasar dev -m capacitor -T android",
    "dev:capacitor:android:ide": "npx quasar dev -m capacitor -T android --ide",
    "build:web": "npm run build:pwa",
    "build:android": "npm run build:capacitor:android:ide",
    "build:pwa": "npx quasar build -m pwa",
    "build:capacitor:android": "npx quasar build -m capacitor -T android",
    "build:capacitor:android:ide": "npx quasar build -m capacitor -T android --ide",
    "cap:sync": "cd src-capacitor && npx cap sync",
    "cap:open:android": "cd src-capacitor && npx cap open android"
  },
  "dependencies": {
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/pwa-elements": "^3.2.2",
    "@quasar/extras": "^1.0.0",
    "@vueuse/sound": "^2.0.1",
    "firebase": "^10.9.0",
    "keen-slider": "^6.8.6",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vue3-charts": "^1.1.33",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "workbox-build": "^7.0.0",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "engines": {
    "node": "^20 || ^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
